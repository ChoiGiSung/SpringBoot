alter table item add constraint item_mount check (item_mount >= 0); //재고가 음수가 되지않는 제약조건

alter table item drop constraint item_mount; // 제약조건 삭제

id bigint generated by default as identity //id 자동 증가

select o.*
from item i,order_table o
where
i.user_name=o.user_name
and i.user_name='기성이'
and o.order_state='cancle'// 서치할떄